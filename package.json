{
  "name": "yolo.js",
  "version": "0.0.1",
  "description": "yolo object detection in javascript",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheHidden1/YOLO.JS.git"
  },
  "scripts": {
    "prebuild": "rimraf dist",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "npm run prebuild && webpack --config webpack.prod.js",
    "lint": "tslint -p . -t verbose",
    "node-build": "tsc && tsc -m es6 --outDir lib-esm"
  },
  "keywords": [
    "yolo",
    "classification",
    "object detection",
    "tfjs",
    "ai",
    "ml"
  ],
  "author": "TheHidden1",
  "license": "MIT",
  "peerDependencies": {
    "@tensorflow/tfjs": "^1.0.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "rimraf": "2.6.2",
    "terser-webpack-plugin": "^1.2.3",
    "tslint": "^5.14.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
}

{
  "name": "@hiddentn/yolo.js",
  "version": "0.0.10",
  "description": "YOLO Object detection in javascript & Nodejs",
  "repository": {
    "type": "git",
    "url": "https://github.com/TheHidden1/YOLO.JS.git"
  },
  "files": [
    "lib/**/*",
    "src/**/*"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "clean": "rimraf dist",
    "start:browser": "webpack-dev-server --open --config webpack.dev.js",
    "build": "npm run clean &&  npm run lint && tsc && webpack --config webpack.prod.js",
    "lint": "tslint -p . -t verbose",
    "prepublishOnly": "npm run build",
    "preversion": "npm run lint",
    "version": "git add -A src",
    "postversion": "git push && git push --tags"
  },
  "bugs": {
    "url": "https://github.com/TheHidden1/YOLO.JS/issues"
  },
  "homepage": "https://github.com/TheHidden1/YOLO.JS#readme",
  "keywords": [
    "YOLO",
    "Classification",
    "Object Detection",
    "TF.js",
    "AI",
    "ML"
  ],
  "author": "TheHidden1",
  "license": "MIT",
  "peerDependencies": {
    "@tensorflow/tfjs": "^1.0.0"
  },
  "devDependencies": {
    "@tensorflow/tfjs": "^1.0.0",
    "@types/node": "^11.12.0",
    "awesome-typescript-loader": "^5.2.1",
    "rimraf": "2.6.2",
    "terser-webpack-plugin": "^1.2.3",
    "tslint": "^5.14.0",
    "tslint-loader": "^3.5.4",
    "typescript": "^3.3.3",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "webpack-merge": "^4.2.1"
  }
